{
  "compilerOptions": {
    "target": "es2016",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "app.ts"],
  "exclude": ["node_modules"]
}
